#include<iostream>
using namespace std;

int main ()

{

 int a, b, c ;

 cout << "Podaj pierwszą liczbę\n";
 cin >> a;
 cout << "Podaj drugą liczbę\n";
 cin >> b;
 cout << "Podaj trzecią liczbę\n";
 cin >> c;
 cout << a << " " << b << " " << c << "\n";
 cout << c << " " << b << " " << a << "\n";

 return 0;

}
